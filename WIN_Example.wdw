              code : |1+
               sResult is string
               
               // Defines the content of the variable dynamically
               MyContact is a JSON
               MyContact.LastName = "JOHNSON"
               MyContact.FirstName = "Helen"
               MyContact.DateOfBirth = "1983-06-12"
               MyContact.PhoneNumber.Fixed = "222-333-4567"
               MyContact.PhoneNumber.CellPhone = "222-333-8901"
               MyContact.Emails = [ "johnson.helen@precilia.com", "hj@precilia.com" ]
               
               // The JSON content can be retrieved in a string variable
               sJSONContent is string = MyContact
               Info("JSON content generated:", sJSONContent)
               
               // It allows you to:
               // - directly access the desired value
               sResult = "The contact's name is [%MyContact.FirstName%] [%MyContact.LastName%]"
               // - check the existence of an element
               if MyContact.PhoneNumber.Fixed..Exist then
               	sResult += [CR] + "Telephone number found"
               else
               	sResult += [CR] + "No telephone number found"
               end
               // - retrieve the number of occurrences of an array
               sResult += [CR] + "The contact has [%MyContact.Emails..Count%] email(s)"
               // - browse the arrays
               FOR ALL ELEMENT sAnEmail of MyContact.Emails
               	sResult += [CR] +"- " + sAnEmail
               end
               Info("Result of access to the elements:", sResult)
               
               
