              code : |1+
               
               // Opens the file picker
               sFile is string = fSelect(fExeDir(), "contact_export.json", "Select a file...", "JSON file (*.json)" + TAB + "*.json", "*.json", fselCreate + fselExist)
               // If no file is selected, no action
               if sFile ~= "" then return
               
               // Starts the export of the file
               if HExportJSON(Contact, sFile) = False then
               	Error("Unable to export the contact file records:", HErrorInfo())
               	return
               end
               
               // Suggests to the user to open the generated file with the default drive
               if Dialog("The contact file records have been exported successfully.[%RC%]Would you like to open the JSON file with the associated default application?", ["Open the file", "Do not open"], 1, 2) = 1 then 
               	ShellExecute(sFile)
               end
