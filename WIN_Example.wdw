            -
              code : |1+
               
               // Stores the number of contacts in the HFSQL file
               let nNbRecord = HNbRec(Contact)
               
               // Imports data from a JSON file into the "contact" HFSQL file
               if HImportJSON(Contact, fExeDir()+[fSep]+"contact_to_import.json") = False then
               	Error("Unable to import the JSON file:", HErrorInfo())
               	return
               end
               
               // End of process
               if nNbRecord = HNbRec(Contact) then
               	ToastDisplay("JSON import completed[%rc%]No record was imported", toastShort, vaTop, haCenter)
               else
               	ToastDisplay("JSON import completed[%RC%][%val(HNbEnr(Contact)-nNbRecord)%] imported record(s)", toastShort, vaTop, haCenter)
               end
