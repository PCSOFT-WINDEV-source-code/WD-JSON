               // Defines the query
               // The "JSON_QUERY" command allows you to retrieve an object or array in JSON format
               // Here, the query is used to retrieve the list of available colors for each product
               sSQLQuery is string = [
               SELECT
               Product.Reference,
               Product.Caption,
               Product.Characteristics,
               JSON_QUERY (Product.Characteristics, '$.colors') AS Colors
               FROM
               Product
               ]
               
               // Runs the query
               dsQuery is data source
               if HExecuteSQLQuery(dsQuery, sSQLQuery) = False then
               	Error("Unable to run the SQL query: ", HErrorInfo())
               	return
               end
               
               
               sResult is string
               nLimit is int = 3 // Limits the display to 3 results
               // Browses the results
               for each dsQuery 
               	sResult += [CR+CR] + "> Product: [%dsQuery.Reference%], [%dsQuery.Caption%][%RC%]Colors: [%dsQuery.Colors%][%RC%]JSON: [%dsQuery.Characteristics%]"
               	
               	// If the limit is reached, stops browsing
               	nLimit--
               	if nLimit <= 0 then break
               end
               
               // Displays the result
               Info("Products with their colors: [%HNbEnr(dsQuery)%] results", "3 first results : ", sResult)
