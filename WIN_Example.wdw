           p_codes :
            -
              internal_properties : CAAAAAgAAADbnYIZ+ZN/lt9sj+o8xyj5Gy2cbOhNg5M3anu4+NXPp8HkVmBriNl9eEODhBAede7/irpwEHQe0NJZXLFsRDIQ+W0QZwHg9FT4upYAYraMFpeVZE8Wz+LThOTxyIP1S7BR8dNNjlhqwrQGJ3w=
              code : |1+
               // Defines the query
               // The "JSON_OBJECT" command allows you to retrieve content in JSON format from any item
               // The retrieved JSON content corresponds to an object ( { xxx,...} )
               sSQLQuery is string = [
               SELECT
               Contact.ContactID,
               JSON_OBJECT ('FullName' : Contact.LastName+' '+Contact.FirstName, 'Address' : Address+' '+ZipCode+' '+City+' '+Country) AS JSONContent
               FROM
               Contact
               WHERE
               ContactID <= 3
               ]
               
               // Runs the query
               dsQuery is data source
               if HExecuteSQLQuery(dsQuery, sSQLQuery) = False then
               	Error("Unable to run the SQL query: ", HErrorInfo())
               	return
               end
               
               // Browses the results
               sResult is string
               for each dsQuery 
               	sResult += [CR+CR] + "Contact # [%dsQuery.ContactID%] :  " + dsQuery.JSONContent
               end
               
               // Displays the result
               Info("Result of the query:", sResult)
              type : 18
            -
              type : 161
              enabled : false
        -
          name : BTN_JSON_OBJECTAGG
          identifier : 0x2c0f9dab054f7040
          internal_properties : 
          properties :
           x : 839
           y : 438
           width : 155
           height : 24
          type : 4
          code_elements :
           type_code : 4
           p_codes :
            -
              code : |1+
               // Defines the query
               // The "JSON_OBJECTAGG" command makes it possible to retrieve a JSON object containing a "key: value" pair for each specific key and value in a set of SQL values.
               // In this example, the pair retrieved is "<LastName FirstName>":"<Complete Address>"
               sSQLQuery is string = [
               SELECT
               JSON_OBJECTAGG (Contact.LastName+' '+Contact.FirstName VALUE Address+' '+ZipCode+' '+City+' '+Country) AS JSONContent
               FROM
               Contact
               WHERE
               ContactID <= 3
               ]
               
               // Runs the query
